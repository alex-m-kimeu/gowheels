{
	"name": "gowheels",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint --no-warn-ignored",
		"format": "prettier --write . --no-editorconfig --loglevel silent",
		"check": "prettier --check . --no-editorconfig",
		"lint-fix": "npm run lint -- --fix",
		"fix": "npm run format && npm run lint-fix",
		"type-check": "tsc --noEmit",
		"clean": "rm -rf .next out dist",
		"clean-install": "npm run clean && rm -rf node_modules package-lock.json && npm install",
		"test": "echo 'No tests specified' && exit 0",
		"prepare": "husky",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate dev"
	},
	"lint-staged": {
		"*": [
			"prettier --check . --no-editorconfig"
		],
		"*.{js,ts,tsx}": [
			"eslint --cache --fix --no-warn-ignored"
		]
	},
	"dependencies": {
		"@hookform/resolvers": "^5.1.1",
		"@prisma/client": "^6.10.1",
		"@radix-ui/react-avatar": "^1.1.10",
		"@radix-ui/react-dialog": "^1.1.14",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-popover": "^1.1.14",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-separator": "^1.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@vercel/analytics": "^1.5.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"date-fns": "^4.1.0",
		"embla-carousel-react": "^8.6.0",
		"framer-motion": "^12.18.1",
		"lucide-react": "^0.515.0",
		"next": "15.3.3",
		"react": "^19.1.0",
		"react-day-picker": "^9.7.0",
		"react-dom": "^19.1.0",
		"react-hook-form": "^7.58.0",
		"react-icons": "^5.5.0",
		"tailwind-merge": "^3.3.1",
		"zod": "^3.25.64"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^24",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.3.3",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.4.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2",
		"prettier": "^3.5.3",
		"prisma": "^6.10.1",
		"tailwindcss": "^4",
		"tw-animate-css": "^1.3.4",
		"typescript": "^5"
	}
}
